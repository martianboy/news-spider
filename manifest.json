{
  "manifest_version": 2,
  "name": "KHU News Crawler",
  "version": "0.0.2",
  "minimum_chrome_version": "36",
  "icons": {
    "128": "images/icon.png",
    "24": "images/icon-24.png"
  },
  "permissions": [
    "http://farsnews.com/"
  ],
  "app": {
    "background": {
      "scripts": ["js/background.js"],
      "persistent": false
    }
  }
}
